    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.0",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.1.1",
    "mkdirp": "^1.0.4",
    "rimraf": "^5.0.10",
    "vite": "^7.1.7"
  }
}
